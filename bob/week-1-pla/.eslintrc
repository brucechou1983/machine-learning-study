{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "mocha": true
  },
  "rules": {
    "no-console": 0,
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "symbol-description": 0,
    "arrow-parens": 0,
    "no-param-reassign": 0
  },
  "globals": {
    "expect": true,
    "assert": true
  }
}
